<ul class="thumbnails">
  <% @decks.each do |deck| %>
    <li class="span3">
      <div class="thumbnail">
        <h4>
          <%= link_to deck.title, user_deck_url(deck.user, deck) %>
          <small>by <%= deck.author %></small>
        </h4>
        <p></p>
      </div>
    </li>
  <% end %>
</ul>